#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/bin/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/bin/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules:/Users/ismaelsilva/Dev-Mini/ismaHOST/ismacopy/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.27.1_@babel+plugin-transform-react-jsx@7.27.1_azjqjqbigrrjnrbuydoxe5htia/node_modules/react-scripts/bin/react-scripts.js" "$@"

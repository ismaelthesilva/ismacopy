{
  "name": "ismacopy-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "cd apps/web && npm run dev",
    "dev:vite": "cd apps/vite && npm run dev",
    "dev:admin": "cd apps/admin && npm run dev",
    "build": "turbo run build",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "packageManager": "npm@10.0.0"
}
